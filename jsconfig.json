{
    "exclude": [
        "node_modules"
    ],
    "compilerOptions": {
        "module": "Node16",
        "baseUrl": "./",
        "paths": {
            "#Config/*": [
                "./src/config/*"
            ],
            "#Controllers/*": [
                "./src/Controllers/*"
            ],
            "#Dto/*": [
                "./src/dto/*"
            ],
            "#Schemas/*": [
                "./src/schemas/*"
            ],
            "#Routes/*": [
                "./src/routes/*"
            ]
        }
    }
}